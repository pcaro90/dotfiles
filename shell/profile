# Start agents
eval $(keychain --eval --agents ssh,gpg)

# set PATH so it includes user's private bin if it exists
PATH="$HOME/utils:$HOME/bin:$HOME/.local/bin:$PATH"

# Python local path
export PYTHONPATH=~/.local

# ZSHrc
if [ -f ~/.zshrc ] ; then
    . ~/.zshrc
fi
